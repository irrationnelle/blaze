{
  "name": "blaze-shortener",
  "version": "0.0.4",
  "type": "module",
  "source": "./index.ts",
  "main": "./index.ts",
  "module": "./lib/index.mjs",
  "types": "./lib/index.d.ts",
  "exports": {
    ".": {
      "require": "./lib/index.cjs",
      "import": "./lib/index.mjs"
    },
    "./package.json": "./package.json"
  },
  "publishConfig": {
    "access": "public",
    "main": "./lib/index.mjs"
  },
  "files": [
    "lib"
  ],
  "scripts": {
    "clean": "rimraf lib",
    "prepack": "yarn clean && yarn build",
    "build": "nanobundle build",
    "test": "jest",
    "test:watch": "jest --watchAll"
  },
  "packageManager": "yarn@3.2.1",
  "jest": {
    "transform": {
      "^.+\\.(t|j)sx?$": [
        "@swc/jest"
      ]
    },
    "transformIgnorePatterns": [
      "node_modules/(?!url-template)"
    ]
  },
  "dependencies": {
    "nanoid": "^3.3.4",
    "typescript": "^4.7.2",
    "url-template": "^3.0.0"
  },
  "devDependencies": {
    "@swc/core": "^1.2.194",
    "@swc/jest": "^0.2.21",
    "@types/lokijs": "^1.5.7",
    "@types/node": "^17.0.35",
    "blaze-test-plugin": "^0.0.2",
    "jest": "^28.1.0",
    "lokijs": "^1.5.12",
    "nanobundle": "^0.0.28",
    "rimraf": "^3.0.2"
  }
}
